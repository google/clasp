{
  "name": "@google/clasp",
  "version": "2.3.1",
  "description": "Develop Apps Script Projects locally",
  "type": "module",
  "exports": "./build/src/index.js",
  "main": "build/src/index.js",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "bin": {
    "clasp": "build/src/index.js"
  },
  "files": [
    "docs",
    "build/src"
  ],
  "scripts": {
    "build": "npm run compile && npm i -g --loglevel=error --force",
    "build-fresh": "npm cache clean --force && npm i && npm run build",
    "watch": "tsc --project tsconfig.json --watch",
    "prepare": "npm run compile",
    "lint": "npm run check",
    "test": "nyc mocha --cache false --timeout 100000 --recursive build/test",
    "coverage": "nyc --cache false report --reporter=text-lcov | coveralls",
    "prettier": "prettier src test --write",
    "check": "gts check src/*.ts src/**/*.ts test/*.ts test/**/*.ts",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix src/*.ts src/**/*.ts test/*.ts test/**/*.ts",
    "pretest": "npm run compile && npm run check"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/google/clasp"
  },
  "keywords": [
    "Apps",
    "Script",
    "SDK",
    "API",
    "script.google.com",
    "extension",
    "add-on"
  ],
  "author": "Grant Timmerman",
  "license": "Apache-2.0",
  "dependencies": {
    "chalk": "^4.1.1",
    "chokidar": "^3.5.1",
    "cli-truncate": "^2.1.0",
    "commander": "^7.2.0",
    "debounce": "^1.2.1",
    "dotf": "^2.0.0",
    "fs-extra": "^10.0.0",
    "fuzzy": "^0.1.3",
    "google-auth-library": "^7.1.0",
    "googleapis": "^74.0.0",
    "inquirer": "^8.1.0",
    "inquirer-autocomplete-prompt-ipt": "^2.0.0",
    "is-reachable": "^5.0.0",
    "log-symbols": "^5.0.0",
    "loud-rejection": "^2.2.0",
    "make-dir": "^3.1.0",
    "multimatch": "^5.0.0",
    "normalize-newline": "^4.1.0",
    "open": "^8.2.0",
    "ora": "^5.4.0",
    "p-map": "^5.0.0",
    "read-pkg-up": "^8.0.0",
    "recursive-readdir": "^2.2.2",
    "split-lines": "^3.0.0",
    "strip-bom": "^5.0.0",
    "ts2gas": "^4.0.0",
    "typescript": "^4.2.4",
    "wtfnode": "^0.8.4"
  },
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/debounce": "^1.2.0",
    "@types/fs-extra": "^9.0.11",
    "@types/inquirer": "^7.3.1",
    "@types/mocha": "^8.2.2",
    "@types/node": "^12.20.13",
    "@types/recursive-readdir": "^2.2.0",
    "@types/tmp": "^0.2.0",
    "@types/wtfnode": "^0.7.0",
    "chai": "^4.3.4",
    "coveralls": "^3.1.0",
    "gts": "^3.1.0",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "prettier": "^2.3.0",
    "tmp": "^0.2.1",
    "type-fest": "^1.1.3"
  }
}
